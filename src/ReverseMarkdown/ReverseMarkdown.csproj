<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>ReverseMarkdown is a Html to Markdown converter library in c#</Description>
    <VersionPrefix>1.0.0</VersionPrefix>
    <Authors>Babu Annamalai</Authors>
    <TargetFrameworks>netcoreapp1.1;netstandard1.6;net46</TargetFrameworks>
    <AssemblyName>ReverseMarkdown</AssemblyName>
    <RootNamespace>ReverseMarkdown</RootNamespace>
    <PackageId>ReverseMarkdown</PackageId>
    <PackageProjectUrl>https://github.com/mysticmind/reversemarkdown-net</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/mysticmind/reversemarkdown-net/blob/master/LICENSE</PackageLicenseUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="HtmlAgilityPack.NetCore" Version="1.5.0.1" />
    <PackageReference Include="Microsoft.AspNetCore.WebUtilities" Version="1.1.2" />
  </ItemGroup>
</Project>